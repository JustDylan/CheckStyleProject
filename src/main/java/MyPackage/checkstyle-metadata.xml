<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE checkstyle-metadata PUBLIC
"-//eclipse-cs//DTD Check Metadata 1.1//EN"
"http://eclipse-cs.sourceforge.net/dtds/checkstyle-metadata_1_1.dtd">
<checkstyle-metadata>
    <rule-group-metadata name="CheckStyleProject 1"  priority="1600">
        <description>new description</description>
        
        <rule-metadata name="Expression count check" internal-name="ExpressionCount" parent="TreeWalker">
            <alternative-name internal-name="MyPackage.ExpressionCountCheck" />
            <description>Counts Expressions</description>
        </rule-metadata>
        
        <rule-metadata name="Halstead length check" internal-name="HalsteadLength" parent="TreeWalker">
            <alternative-name internal-name="MyPackage.HalsteadLengthCheck" />
            <description>Calculates the Halstead length of a program</description>
        </rule-metadata>
        
        <rule-metadata name="Number of operators check" internal-name="NumOperators" parent="TreeWalker">
            <alternative-name internal-name="MyPackage.NumOperatorsCheck" />
            <description>Counts number of operators in a program</description>
        </rule-metadata>
        
        <rule-metadata name="Halstead vocabulary check" internal-name="HalsteadVocabulary" parent="TreeWalker">
            <alternative-name internal-name="MyPackage.HalsteadVocabularyCheck" />
            <description>Counts unique number of operators and operands</description>
        </rule-metadata>
        
        <rule-metadata name="Halstead volume check" internal-name="HalsteadVolume" parent="TreeWalker">
            <alternative-name internal-name="MyPackage.HalsteadVolumeCheck" />
            <description>Computes Halstead Volume</description>
        </rule-metadata>
        
        <rule-metadata name="Halstead difficulty check" internal-name="HalsteadDifficulty" parent="TreeWalker">
            <alternative-name internal-name="MyPackage.HalsteadDifficultyCheck" />
            <description>Computes Halstead Difficulty</description>
        </rule-metadata>
        
        <rule-metadata name="Halstead effort check" internal-name="HalsteadEffort" parent="TreeWalker">
            <alternative-name internal-name="MyPackage.HalsteadEffortCheck" />
            <description>Computes halstead effort</description>
        </rule-metadata>
        
        <rule-metadata name="Number of operands check" internal-name="NumOperands" parent="TreeWalker">
            <alternative-name internal-name="MyPackage.NumOperandsCheck" />
            <description>Counts total number of operands</description>
        </rule-metadata>
        
        <rule-metadata name="Looping statement count check" internal-name="LoopCount" parent="TreeWalker">
            <alternative-name internal-name="MyPackage.LoopCountCheck" />
            <description>Counts total number of looping statements</description>
        </rule-metadata>
        
        <rule-metadata name="Number of comments check" internal-name="NumComments" parent="TreeWalker">
            <alternative-name internal-name="MyPackage.NumCommentsCheck" />
            <description>Counts total number of comments</description>
        </rule-metadata>
        
        <rule-metadata name="Comment line count check" internal-name="CommentLineCount" parent="TreeWalker">
            <alternative-name internal-name="MyPackage.CommentLineCountCheck" />
            <description>Counts number of lines with comments</description>
        </rule-metadata>
        
        <rule-metadata name="%MethodLimit.name" internal-name="MethodLimit" parent="TreeWalker">
            <alternative-name internal-name="MyPackage.MethodLimitCheck" />
            <description>%MethodLimit.desc</description>
            <property-metadata name="max" datatype="Integer" default-value="30">
                <description>The maximum number of allowed methods.</description>
            </property-metadata>
        </rule-metadata>
    </rule-group-metadata>
</checkstyle-metadata>
